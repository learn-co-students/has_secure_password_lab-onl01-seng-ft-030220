<h1>New User Sign Up</h1><br>

<%= form_for @user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :password %>
    <%= f.password_field :password %>
    
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
    
    <%= f.submit "Create New User" %>
<% end %>